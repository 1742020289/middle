{
  "name": "goodo-midtier-v2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "serve": "node --max-old-space-size=8000 -r ts-node/register -r tsconfig-paths/register -r dotenv/config --trace-warnings ./src/main.ts",
    "build": "webpack --env=production --mode=production --config webpack.config.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm i && npm run pm2:kill && npm run pm2:start",
    "pm2:start": "pm2 start pm2.json",
    "pm2:test": "npm run pm2:start",
    "pm2:kill": "pm2 kill",
    "pm2:stop": "pm2 stop all",
    "pm2:log": "pm2 logs",
    "pm2:update": "pm2 update",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "clone": "ts-node -r tsconfig-paths/register ./scripts/clone.ts",
    "deploy": "npm run build && npm run clone -- --mode production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.1",
    "app-root-path": "^3.0.0",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "md5": "^2.3.0",
    "mddir": "^1.1.1",
    "pm2": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/md5": "^2.3.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@types/webpack-node-externals": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "moment": "^2.29.1",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.3",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
